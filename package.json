{
  "name": "starter",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=en",
    "start:fr": "ng serve --configuration=fr",
    "build": "ng build",
    "build:fr": "ng build --configuration=production-fr",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs:technical": "compodoc -p ./tsconfig.compodoc.app.json --output documentation-technical --hideGenerator",
    "build:stats": "ng build --stats-json",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll=false",
    "postinstall": "ngcc",
    "publish-lib": "node publish.js publish",
    "unpublish-lib": "node publish.js unpublish",
    "generate-lib": "node generator.js",
    "extract": "ng extract-i18n --output-path=src/locale"
  },
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "ng lint",
      "pre-push": "ng test --watch false"
    }
  },
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/cdk": "^11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/forms": "~11.1.1",
    "@angular/material": "^11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/router": "~11.1.1",
    "@testing-library/user-event": "^12.6.3",
    "ng-packagr": "^11.1.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^11.0.0",
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/cli": "~11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/localize": "^11.1.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@compodoc/ts-stats": "^1.1.1",
    "@testing-library/angular": "^10.3.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.15",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "codelyzer": "^6.0.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "jest-preset-angular": "^8.3.2",
    "karma-coverage": "~2.0.3",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "protractor": "~7.0.0",
    "sass-lint": "^1.10.2",
    "sass-lint-auto-fix": "^0.16.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  }
}
